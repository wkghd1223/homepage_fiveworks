  <script>
    var typed = new Typed('.typed-words', {
      strings: ["A.I", " Big Data", " S.I"],
      typeSpeed: 80,
      backSpeed: 80,
      backDelay: 4000,
      startDelay: 1000,
      loop: true,
      showCursor: true
    });

    var container = document.getElementById('map');
    var options = {
      center: new kakao.maps.LatLng(37.56039, 126.82737),
      level: 3
    };
    var map = new kakao.maps.Map(container, options);
    var marker = new kakao.maps.Marker({
      // 지도 중심좌표에 마커를 생성합니다 
      position: map.getCenter()
    });
    // 지도에 마커를 표시합니다
    marker.setMap(map);
  </script>

